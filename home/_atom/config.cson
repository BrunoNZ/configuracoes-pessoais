"*":
  "atom-material-ui":
    colors:
      abaseColor: "#7e7e7e"
      predefinedColor: "Grey"
    fonts: {}
    tabs:
      compactTabs: true
      tintedTabBar: true
    treeView:
      blendTabs: true
      compactList: true
    ui:
      animations: false
      panelContrast: true
  core:
    audioBeep: false
    autoHideMenuBar: true
    automaticallyUpdate: false
    disabledPackages: [
      "language-objective-c"
      "background-tips"
      "encoding-selector"
      "language-clojure"
      "language-go"
      "language-mustache"
      "language-perl"
      "language-php"
      "language-toml"
      "metrics"
      "open-on-github"
      "release-notes"
      "autoflow"
      "dev-live-reload"
      "grammar-selector"
      "language-gfm"
      "welcome"
      "language-csharp"
      "package-generator"
      "language-opencl"
      "command-palette"
      "build-make"
      "busy"
      "build"
      "minimap"
    ]
    telemetryConsent: "limited"
  editor:
    autoIndentOnPaste: false
    fontFamily: "Ubuntu Mono"
    fontSize: 22
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    tabLength: 4
    tabType: "soft"
    zoomFontWhenCtrlScrolling: false
  "exception-reporting":
    userId: "8e4c8905-ae31-1a14-9e27-d9fd7d839909"
  "file-icons":
    strategies:
      grammar: false
      hashbangs: false
      modelines: false
      usertypes: false
  metrics: false
  minimap:
    plugins:
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  "one-dark-ui":
    hideDockButtons: true
  "spell-check":
    addKnownWords: true
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.tex.latex"
    ]
    knownWords: [
      "chapter"
      "section"
      "label"
      "ref"
      "sec"
      "fig"
      "subsection"
      "phoenix"
      "PHOENIX"
      "begin"
      "end"
      "caption"
      "includegraphics"
      "width"
      "centering"
      "small"
      "algorithm"
      "algorithmic"
      "input"
      "subsubsection"
    ]
    localePaths: [
      "/home/bruno/.local/share/dicts"
    ]
    locales: [
      "pt-BR"
    ]
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
